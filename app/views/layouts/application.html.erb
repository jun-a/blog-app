<!DOCTYPE html>
<html>
<head>
  <title>RailsBaseProject</title>
  <%= stylesheet_link_tag    'application', media: 'all' %>
  <%= javascript_include_tag 'application' %>
  <%= csrf_meta_tags %>
</head>
<body>

<%= yield %>
	<!--login_session-->
	<!--sessionからの名前の変更は必要か？-->
	<% if current_user == nil %>
  		<%= link_to 'Login', 'signin' %>
	<% else %>
  		<%= link_to 'Logout', 'signout', :confirm => 'ログアウトしますか？', :method => :delete %>
	<% end %>
	

</body>
</html>
